<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>Načítavanie...</string>

	<!-- OK button label. -->
	<key>OK</key>
	<string>OK</string>

	<!-- Title of prompt asking the user to enable the extension. -->
	<key>PLEASE_ENABLE</key>
	<string>Pre začatie používania TermHere, prosím zapnite rozšírenie.</string>

	<!-- Explanation of how to enable the extension. -->
	<key>PLEASE_ENABLE_EXPLANATION</key>
	<string>Kliknite OK pre otvorenie Rozšírenia. Pod TermHere odkliknite "Finder".</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>Vybrať</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>Vybrať aplikáciu</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>O TermHere</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>Nebolo možné spustiť aplikáciu kvôli neznámej chybe.</string>
</dict>
</plist>
