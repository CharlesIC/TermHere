<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>Ladataan…</string>

	<!-- OK button label. -->
	<key>OK</key>
	<string>OK</string>

	<!-- Title of prompt asking the user to enable the extension. -->
	<key>PLEASE_ENABLE</key>
	<string>Laita laajennus päälle aloittaaksesi TermHere:n käytön.</string>

	<!-- Explanation of how to enable the extension. -->
	<key>PLEASE_ENABLE_EXPLANATION</key>
	<string>Klikkaa OK avataksesi Laajennukset-asetukset. Klikkaa valintalaatikkoa "Finder":in vieressä TermHere:n alla.</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>Valitse</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>Valitse ohjelma</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>Tietoja</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>Ohjelmaa ei voitu avata tuntemattoman virheen takia.</string>
</dict>
</plist>
