<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>Laden…</string>

	<!-- OK button label. -->
	<key>OK</key>
	<string>OK</string>

	<!-- Title of prompt asking the user to enable the extension. -->
	<key>PLEASE_ENABLE</key>
	<string>Activeer A.U.B. de extentie om TermHere te gebruiken.</string>

	<!-- Explanation of how to enable the extension. -->
	<key>PLEASE_ENABLE_EXPLANATION</key>
	<string>Klik op OK om de Extentie instellingen te openen, dan vervolgends onder TermHere, activeer de checkbox naast "Finder"</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>Kies</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>Kies Applicatie</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>Over</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>Kon de Applicatie niet laden omdat er een onbekende fout is opgetreden.</string>
</dict>
</plist>
