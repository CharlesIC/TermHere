<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label displayed while something is loading. -->
	<key>LOADING</key>
	<string>Načítání…</string>

	<!-- OK button label. -->
	<key>OK</key>
	<string>OK</string>

	<!-- Title of prompt asking the user to enable the extension. -->
	<key>PLEASE_ENABLE</key>
	<string>Pro použití TermHere, prosím aktivujte doplněk.</string>

	<!-- Explanation of how to enable the extension. -->
	<key>PLEASE_ENABLE_EXPLANATION</key>
	<string>Klikněte "OK" pro otevření nastavení pro doplňky. Pod TermHere, zaškrtněte na políčko vedle "Finder".</string>

	<!-- Button that chooses the selected app in the open panel. -->
	<key>CHOOSE</key>
	<string>Vybrat</string>

	<!-- Title of the open panel. -->
	<key>CHOOSE_APPLICATION</key>
	<string>Vyberte aplikaci</string>

	<!-- Title of the About window. -->
	<key>ABOUT</key>
	<string>O aplikaci</string>

	<!-- Message displayed when the app fails to be opened. -->
	<key>OPENING_APP_FAILED</key>
	<string>Nebylo možné sputit aplikaci kvůli neznámé chybě.</string>
</dict>
</plist>
